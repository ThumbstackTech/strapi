{
  "kind": "collectionType",
  "collectionName": "final_igypsy_destinations",
  "info": {
    "singularName": "final-igypsy-destination",
    "pluralName": "final-igypsy-destinations",
    "displayName": "final_igypsy_destination",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Destination_Code_City": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Content_code": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "Cost": {
      "type": "biginteger"
    },
    "Time_Needed_in_hrs": {
      "type": "integer"
    },
    "Public_Transport": {
      "type": "string",
      "required": true
    },
    "cab": {
      "type": "enumeration",
      "enum": [
        "Available",
        "Not_Available",
        "Easy_to_Get",
        "Hard_to_Get"
      ],
      "required": true,
      "default": "Available"
    },
    "Location_Latitude": {
      "type": "float",
      "required": true
    },
    "Location_Longitude": {
      "required": true,
      "type": "float"
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Location_Url": {
      "type": "string",
      "required": true
    },
    "FoodType": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::name.name",
      "mappedBy": "final_igypsy_destination"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "final_igypsy_destination"
    },
    "links": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::link.link",
      "mappedBy": "final_igypsy_destinations"
    },
    "Content": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "ExpertRating": {
      "type": "integer",
      "required": true,
      "max": 5,
      "min": 1
    },
    "SocialRating": {
      "type": "integer",
      "max": 5,
      "required": true,
      "min": 1
    },
    "Introduction": {
      "type": "richtext",
      "required": true
    },
    "NameOfPlace": {
      "type": "string",
      "required": true
    },
    "theaters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::theater.theater",
      "mappedBy": "final_igypsy_destination"
    },
    "sight_seeings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sight-seeing.sight-seeing",
      "mappedBy": "final_igypsy_destination"
    },
    "tours": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tour.tour",
      "mappedBy": "final_igypsy_destination"
    }
  }
}
